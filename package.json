{
	"name": "kickstart-sveltekit",
	"version": "0.0.1",
	"type": "module",
	"private": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/contentstack/kickstart-sveltekit"
	},
	"description": "Contentstack Kickstarts are the minimum amount of code needed to connect to Contentstack.",
	"author": "Tim Benniks <tim.benniks@contentstack.com>",
	"homepage": "https://contentstack.com",
	"license": "MIT",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.1",
		"@sveltejs/kit": "^2.21.5",
		"@sveltejs/vite-plugin-svelte": "^5.1.0",
		"@tailwindcss/postcss": "^4.1.10",
		"@types/eslint": "^9.6.1",
		"eslint": "^9.29.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-svelte": "^3.9.2",
		"globals": "^16.2.0",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.12",
		"svelte": "^5.34.3",
		"svelte-check": "^4.2.1",
		"tailwindcss": "^4.1.10",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.34.1",
		"vite": "^6.3.5"
	},
	"dependencies": {
		"@contentstack/delivery-sdk": "^4.7.1",
		"@contentstack/live-preview-utils": "^3.2.4"
	}
}
